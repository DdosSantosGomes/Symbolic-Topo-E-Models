{
    "[haskell]": {
        "editor.defaultFormatter": "haskell.haskell",
        "editor.formatOnSave": true,
        "editor.rulers": [
            80,
            120
        ],
        "editor.tabSize": 4
    },
    "[literate haskell]": {
        "editor.rulers": [
            80,
            120
        ]
    },
    "cSpell.enableFiletypes": [
        "literate haskell",
        "!haskell",

    ],
    "cSpell.language": "en-GB",
    "cSpell.words": [
        "Alexandrov",
        "Alexandru",
        "Baltag",
        "bigcup",
        "biginter",
        "bigunion",
        "bisimilar",
        "bisimulation",
        "bisimulations",
        "clopen",
        "closeds",
        "compin",
        "elem",
        "Fira",
        "fmap",
        "foldr",
        "footline",
        "footlines",
        "ghci",
        "Hasse",
        "Kripke",
        "Lato",
        "mathbf",
        "Nbds",
        "oneof",
        "Palatino",
        "pdftitle",
        "ponens",
        "setminus",
        "Subbases",
        "subbasis",
        "subseteq",
        "textbf",
        "topo",
        "topomodel",
        "topomodels",
        "toposemantics",
        "topospace",
        "topospaces",
        "uncurry",
        "varphi"
    ],
    "latex-workshop.latex.autoBuild.run": "onSave",
    "latex-workshop.latex.outDir": "%WORKSPACE_FOLDER%/docs"
}